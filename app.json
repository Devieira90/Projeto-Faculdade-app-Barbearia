{
  "expo": {
    "name": "MeuApp",
    "slug": "MeuApp",
    "version": "1.0.2",
    "orientation": "portrait",
    "icon": "./assets/barbeiro.png",
    "userInterfaceStyle": "light",
    "newArchEnabled": true,
    "splash": {
      "image": "./assets/logo.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.anonymous.MeuApp"
    },
    "android": {
      "versionCode": 4,
      "adaptiveIcon": {
        "foregroundImage": "./assets/adaptive-icon.png",
        "backgroundColor": "#ffffff"
      },
      "edgeToEdgeEnabled": true,
      "package": "com.anonymous.MeuApp"
    },
    "web": {
      "favicon": "./assets/favicon.png"
    },
    "plugins": [
      "expo-maps"
    ],
    "extra": {
      "eas": {
        "projectId": "6544fe3c-b142-46b8-948f-b2d918bdb02f"
      }
    }
  }
}
